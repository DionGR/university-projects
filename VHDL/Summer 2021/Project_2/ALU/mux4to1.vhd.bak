LIBRARY ieee;
USE ieee.std_logic_1164.all;

ENTITY mux4to1 IS
	PORT (in1, in2, in3, in4 : IN STD_LOGIC ;
			s 						 : IN STD_LOGIC_VECTOR(1 DOWNTO 0);
			res 					 : OUT STD_LOGIC);
END mux4to1 ;

ARCHITECTURE mux4to1_behavior OF mux4to1 IS
BEGIN
	WITH s SELECT
	res <= w0 WHEN "00",
	w1 WHEN "01",
	w2 WHEN "10",
	w3 WHEN OTHERS;
END mux4to1_behavior;